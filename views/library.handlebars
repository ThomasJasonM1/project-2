<table>
  <tr>
    <th>Barcode</th>
    <th>Description</th>
    <th>Active</th>
  </tr>
  {{#each barcode}} 
  {{!-- {{#if Active}} --}}
  <tr id="{{Upc}}">
    <td class="upc">{{Upc}}</td>
    <td><textarea class="text" maxlength="50" placeholder="{{Description}}"></textarea></td>
    <td class="status">{{Active}}</td>
    <td><button class="btn btn-default" data-id="{{Upc}}">Save</button></td>
  </tr>
  {{!-- {{/if}}  --}}
  {{/each}}
</table>
